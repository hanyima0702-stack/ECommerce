{
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/search",
      "destination": "/product_search_page/code.html"
    },
    {
      "source": "/cart",
      "destination": "/cart_manage_page/code.html"
    },
    {
      "source": "/checkout",
      "destination": "/checkout_process_page/code.html"
    },
    {
      "source": "/orders",
      "destination": "/order_track_page/code.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}